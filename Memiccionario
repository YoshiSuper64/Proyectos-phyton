meme_dict = {
            "CRINGE": "Algo excepcionalmente raro o embarazoso",
            "LOL": "Una respuesta común a algo gracioso",
            "BASADO": "Cuando dices algo con lo que la mayoria esta de acuerdo",
            "PAPEADO": "Palabra usada cuando alguien humilla a otra persona",
            "PRO": "Abreviatura de profesional",
            "NOOB": "Abreviatura de novato",
            "XD": "Respuesta comun a algo gracioso",
            "SHEESH": "Una ligera desaprovacion",
            "CREEPY": "Aterrador, siniestro",
            "ROFL": "una respuesta a una broma",
            }
word = input("Escribe una palabra que no entiendas (¡con mayúsculas!): ")
if word in meme_dict.keys():
                print (meme_dict[word])
else:
    print("Lo sentimos esta palabra no se encuentra en el diccionario T_T")
